{
  "name": "react-web-chat",
  "version": "1.1.0",
  "description": "A react-based instant messaging interface.",
  "main": "build/index.js",
  "scripts": {
    "dev": "watch 'babel src -d build --source-maps' src",
    "build": "babel src -d build",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "prepublish": "npm run build",
    "release": "np"
  },
  "babel": {
    "presets": [
      "latest",
      "react",
      "stage-2"
    ],
    "plugins": [
      "styled-jsx/babel"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/praekelt/react-web-chat.git"
  },
  "keywords": [
    "react",
    "chat"
  ],
  "author": "Peter Ringelmann",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/praekelt/react-web-chat/issues"
  },
  "homepage": "https://github.com/praekelt/react-web-chat#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "jest": "^21.2.1",
    "np": "^2.16.0",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "styled-jsx": "^2.1.2"
  }
}
